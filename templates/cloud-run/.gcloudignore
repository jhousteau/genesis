# Genesis Cloud Run Ignore
# This file specifies files that are uploaded to Google Cloud Run

# Git
.git/
.gitignore

# Documentation
README.md
*.md
docs/

# Development tools
.vscode/
.idea/
*.code-workspace
.devcontainer/

# Python development
__pycache__/
*.py[cod]
*$py.class
*.so
.pytest_cache/
.coverage
htmlcov/
.tox/
.mypy_cache/

# Virtual environments
.env
.venv
env/
venv/
ENV/

# Node.js
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
yarn.lock

# Build outputs (Cloud Run uses Docker builds)
build/
dist/
out/
target/

# Test files
test/
tests/
*_test.*
*.test.*
*.spec.*

# Logs
*.log
logs/

# Temporary files
*.tmp
*.temp
tmp/
temp/

# OS files
.DS_Store
Thumbs.db

# Secrets (use Secret Manager)
.secrets
*.pem
*.key
*.crt
credentials.json
service-account*.json

# CI/CD files
.github/
.gitlab-ci.yml
.travis.yml
.circleci/

# Terraform files
*.tf
*.tfvars
.terraform/

# Local environment files
.env.local
.env.development
.env.staging
.env.production

# Backup files
*.backup
*.bak
*.orig

# IDE history
.history/

# Cache directories
.cache/
.npm/

# Coverage reports
coverage/
.nyc_output
